<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE troubleshooting PUBLIC "-//OASIS//DTD DITA 1.3 Troubleshooting//EN" "troubleshooting.dtd">
<troubleshooting xml:lang="en-US" id="trbl_error-membrane-t2">
	<title>Error: Membrane T<keyword keyref="group-num-f"/></title>
	<troublebody>
		<troubleSolution>
			<cause>
				<title>Sensor on suction side shows too high a negative pressure (vacuum)</title>
			</cause>
			<remedy>
				<steps-unordered>
					<step>
						<cmd>Check tube for buckling</cmd>
					</step>
					<step>
						<cmd>Membrane is blocked. Please observe the Section “Maintenance” of the Operation Manual.</cmd>
					</step>
				</steps-unordered>
			</remedy>
		</troubleSolution>
	</troublebody>
</troubleshooting>
