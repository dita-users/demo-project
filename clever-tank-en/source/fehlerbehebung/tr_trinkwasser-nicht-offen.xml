<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE troubleshooting PUBLIC "-//OASIS//DTD DITA 1.3 Troubleshooting//EN" "troubleshooting.dtd">
<troubleshooting xml:lang="en-US" id="trbl_trinkwasser-nicht-offen">
	<title>Trinkwasser nicht offen</title>
	<troublebody>
		<troubleSolution>
			<cause>
				<title>Undichtigkeit in der Druckleitung</title>
			</cause>
			<remedy>
				<steps-unordered>
					<step>
						<cmd>Druckleitung kontrollieren</cmd>
					</step>
				</steps-unordered>
			</remedy>
		</troubleSolution>
		<troubleSolution>
			<cause>
				<title>Fehlerhaft angeschlossen</title>
			</cause>
			<remedy>
				<steps-unordered>
					<step>
						<cmd>Klemmplan kontrollieren</cmd>
					</step>
					<step conref="../warehouse/reuse_trbl-elements.xml#trbl_ubx_fxm_n1b/reuse_step-el-test">
						<cmd/>
					</step>
				</steps-unordered>
			</remedy>
		</troubleSolution>
		<troubleSolution>
			<cause>
				<title>Fehler in der Durchflussüberwachung</title>
			</cause>
			<remedy>
				<steps-unordered>
					<step>
						<cmd>Durchflusswächter kontrollieren</cmd>
					</step>
					<step conref="../warehouse/reuse_trbl-elements.xml#trbl_ubx_fxm_n1b/reuse_step-el-test">
						<cmd/>
					</step>
				</steps-unordered>
			</remedy>
		</troubleSolution>
		<troubleSolution>
			<cause>
				<title>Kugelhahn geschlossen</title>
			</cause>
			<remedy>
				<steps-unordered>
					<step>
						<cmd>Kugelhahn öffnen</cmd>
					</step>
				</steps-unordered>
			</remedy>
		</troubleSolution>
	</troublebody>
</troubleshooting>
