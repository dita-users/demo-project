<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE troubleshooting PUBLIC "-//OASIS//DTD DITA 1.3 Troubleshooting//EN" "troubleshooting.dtd">
<troubleshooting xml:lang="en-US" id="trbl_fehler-schwimmer-9sx">
	<title>Fehler Schwimmer 9SX</title>
	<troublebody>
		<condition>
			<p/>
		</condition>
		<troubleSolution>
			<cause>
				<title>unlogischer Reihenfolge der Schwimmer im Tank<keyword keyref="group-num-f"/>/Vorratsbehälter</title>
				<p>z.B.: wenn 9S5 aus ist kann 9S6 nicht an sein</p>
			</cause>
			<remedy>
				<responsibleParty/>
				<steps-unordered>
					<step>
						<cmd>Schwimmer auf Verschmutzung kontrollieren oder elektrisch Prüfen</cmd>
					</step>
				</steps-unordered>
			</remedy>
		</troubleSolution>
	</troublebody>
</troubleshooting>
