<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE troubleshooting PUBLIC "-//OASIS//DTD DITA 1.3 Troubleshooting//EN" "troubleshooting.dtd">
<troubleshooting xml:lang="en-US" id="trbl_error-collector">
	<title>Error: Collector</title>
	<troublebody>
		<troubleSolution>
			<cause>
				<title>Tripping of a fuse in control box</title>
			</cause>
			<remedy>
				<steps>
					<step>
						<cmd>Refer to circuit diagram</cmd>
					</step>
					<step>
						<cmd>Check electric circuit</cmd>
					</step>
					<step>
						<cmd>Electric test</cmd>
					</step>
				</steps>
			</remedy>
		</troubleSolution>
	</troublebody>
</troubleshooting>
