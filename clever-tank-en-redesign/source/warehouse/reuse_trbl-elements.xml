<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE troubleshooting PUBLIC "-//OASIS//DTD DITA 1.3 Troubleshooting//EN" "troubleshooting.dtd">
<troubleshooting xml:lang="en-US" id="trbl_ubx_fxm_n1b">
	<title>Troubleshooting warehouse</title>
	<troublebody>
		<troubleSolution id="reuse_trbl-leaking-tube">
			<cause>
				<title>Leaking tube</title>
			</cause>
			<remedy>
				<steps-unordered>
					<step id="reuse_step-el-test">
						<cmd>Run electrical test</cmd>
					</step>
				</steps-unordered>
			</remedy>
		</troubleSolution>
		<troubleSolution id="reuse_trbl-def-sensor">
			<cause>
				<title>Defective sensor</title>
			</cause>
			<remedy>
				<steps-unordered>
					<step>
						<cmd>Check sensor, if necessary replace</cmd>
					</step>
					<step conref="../warehouse/reuse_trbl-elements.xml#trbl_ubx_fxm_n1b/reuse_step-el-test">
						<cmd/>
					</step>
				</steps-unordered>
			</remedy>
		</troubleSolution>
		<troubleSolution id="reuse_trbl-def-compr">
			<cause>
				<title>Compressor not functioning</title>
			</cause>
			<remedy>
				<steps-unordered>
					<step>
						<cmd>Check supply voltage in manual mode</cmd>
					</step>
					<step conref="../warehouse/reuse_trbl-elements.xml#trbl_ubx_fxm_n1b/reuse_step-el-test">
						<cmd/>
					</step>
				</steps-unordered>
			</remedy>
		</troubleSolution>
	</troublebody>
</troubleshooting>
