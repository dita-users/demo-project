<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE troubleshooting PUBLIC "-//OASIS//DTD DITA 1.3 Troubleshooting//EN" "troubleshooting.dtd">
<troubleshooting xml:lang="de-DE" id="trbl_fehler-membran-t2">
	<title>Fehler Membran T<keyword keyref="group-num-f"/></title>
	<troublebody>
		<troubleSolution>
			<cause>
				<title>Sensor an der Saugseite zeigt einen zu hohen Unterdruck an</title>
			</cause>
			<remedy>
				<steps-unordered>
					<step>
						<cmd>Schlauch auf Knickstellen kontrollieren</cmd>
					</step>
					<step>
						<cmd>Membran hat sich zugesetzt. Bitte den Punkt Wartung in der Bedienungsanleitung beachten.</cmd>
					</step>
				</steps-unordered>
			</remedy>
		</troubleSolution>
	</troublebody>
</troubleshooting>
